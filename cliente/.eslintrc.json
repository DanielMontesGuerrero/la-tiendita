{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "modules": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "require-jsdoc": 0,
        "react/forbid-prop-types": 0,
        "quotes": "off",
        "comma-dangle": "off",
        "no-invalid-this": "off",
        "padded-blocks": "off",
        "keyword-spacing": "off",
        "space-before-blocks": "off",
        "semi": "off",
        "space-before-function-paren": "off",
        "no-multi-spaces": "off",
        "no-unused-vars": "off",
        "new-cap": "off",
        "prefer-const": "off",
        "indent": "off",
        "max-len": "off",
        "spaced-comment": "off"

    }
}
